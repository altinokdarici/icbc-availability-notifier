{
  "bindings": [
    {
      "name": "functionTimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "* * */4 * * *"
    },
    {
      "name": "storedAppointmentDateInput",
      "type": "blob",
      "path": "container/earliest-appointment-date",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "name": "storedAppointmentDateOutput",
      "type": "blob",
      "path": "container/earliest-appointment-date",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "scriptFile": "../dist/FindEarliestAvailableAppointment/index.js"
}
